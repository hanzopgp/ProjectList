{
    // Utilisez IntelliSense pour en savoir plus sur les attributs possibles.
    // Pointez pour afficher la description des attributs existants.
    // Pour plus d'informations, visitez : https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Prolog Files",
            "type": "prolog",
            "request": "launch",
            "runtimeExecutable": "C:/Program files/swipl/bin/swipl",
            "runtimeArgs": [],
            "env": {},
            "program": "${file}",
            "cwd": "${fileDirname}",
            "startupQuery": "start",
            "stopOnEntry": true,
            "traceCmds": {
                "continue": [
                    "leap",
                    "l"
                ],
                "stepover": [
                    "skip",
                    "s"
                ],
                "stepinto": [
                    "creep",
                    "c"
                ],
                "stepout": [
                    "up",
                    "u"
                ]
            }
        }
    ]
}